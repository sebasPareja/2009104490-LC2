@model IEnumerable<LineaTelefonica.Entities.Entities.Venta>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CentroAtencion.NombreCentroAtencion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Evaluacion.EvaluacionId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LineaTelefonica.LineaTelefonicaId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TipoPago.NombreTipoPago)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trabajador.TrabajadorId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CentroAtencion.NombreCentroAtencion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Evaluacion.EvaluacionId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LineaTelefonica.LineaTelefonicaId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TipoPago.NombreTipoPago)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Trabajador.TrabajadorId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.VentaId }) |
            @Html.ActionLink("Details", "Details", new { id=item.VentaId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.VentaId })
        </td>
    </tr>
}

</table>
